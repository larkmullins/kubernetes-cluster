    --client-key=/etc/security/kubernetes/kube-scheduler-key.pem --embed-certs=true --token=4MWMHEKVEeFXqGb1iXqB7yDwofQ4XrMs \
    --kubeconfig=/var/lib/kubernetes/kube-scheduler/kubeconfig
/usr/local/sbin/kubectl config set-context k8s.cluster.local --cluster=k8s.cluster.local --user=kube-scheduler \
    --kubeconfig=/var/lib/kubernetes/kube-scheduler/kubeconfig